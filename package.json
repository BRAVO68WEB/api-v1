    "scripts": {
        "start": "node .",
        "test": "jest",
        "prettier": "prettier --write .",
        "coverage": "npm test -- --coverage",
        "postcoverage": "opn coverage/lcov-report/index.html",
        "dev": "cross-env NODE_ENV=development nodemon -i \"*.test.js\" .",
        "start:doppler": "doppler run -- nodemon -i \"*.test.js\" .",
        "prod": "cross-env NODE_ENV=production nodemon -x \" node . || touch app.js\" -i \"*.test.js\" -r dotenv-safe/config .",
        "lint": "eslint src",
        "docs": "apidoc -i src -o docs && apidoc-markdown -p docs -o DOCS.md",
        "postdocs": "open-cli docs/index.html"
    },
    "jest": {
        "testEnvironment": "node",
        "setupFilesAfterEnv": [
            "<rootDir>/test/setup.js"
        ]
    },
